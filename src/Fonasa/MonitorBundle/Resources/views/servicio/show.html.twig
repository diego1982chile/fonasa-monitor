{% extends 'base.html.twig' %}

{% block body %}
    <h1>Servicio</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ servicio.id }}</td>
            </tr>
            <tr>
                <th>Codigointerno</th>
                <td>{{ servicio.codigoInterno }}</td>
            </tr>
            <tr>
                <th>Codigoexterno</th>
                <td>{{ servicio.codigoExterno }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ servicio.descripcion }}</td>
            </tr>
            <tr>
                <th>Fechaingreso</th>
                <td>{% if servicio.fechaIngreso %}{{ servicio.fechaIngreso|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechasalida</th>
                <td>{% if servicio.fechaSalida %}{{ servicio.fechaSalida|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Anyo</th>
                <td>{{ servicio.anyo }}</td>
            </tr>
            <tr>
                <th>Mes</th>
                <td>{{ servicio.mes }}</td>
            </tr>
            <tr>
                <th>Hhestimadas</th>
                <td>{{ servicio.hhEstimadas }}</td>
            </tr>
            <tr>
                <th>Hhefectivas</th>
                <td>{{ servicio.hhEfectivas }}</td>
            </tr>
            <tr>
                <th>Idcomponente</th>
                <td>{{ servicio.idComponente }}</td>
            </tr>
            <tr>
                <th>Idorigen</th>
                <td>{{ servicio.idOrigen }}</td>
            </tr>
            <tr>
                <th>Idtipo</th>
                <td>{{ servicio.idTipo }}</td>
            </tr>
            <tr>
                <th>Idestado</th>
                <td>{{ servicio.idEstado }}</td>
            </tr>
            <tr>
                <th>Idprioridad</th>
                <td>{{ servicio.idPrioridad }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('servicio_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('servicio_edit', { 'id': servicio.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
